{
  "name": "the-legend-of-zelda",
  "version": "0.0.1",
  "description": "A TypeScript Remake of the original Tloz for nes",
  "main": "index.html",
  "scripts": {
    "build": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "start": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline --watch",
    "check-types": "tsc",
    "check-types:watch": "tsc --watch",
    "test": "NODE_ENV=development jest src",
    "coverage": "jest --coverage"
  },
  "keywords": [
    "nes",
    "thelegendofzelda",
    "rewrite",
    "typescript"
  ],
  "author": "Michael Walker",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/types": "^7.5.0",
    "@types/jest": "^24.0.15",
    "@types/js-yaml": "^3.12.1",
    "@types/node": "^12.6.2",
    "i": "^0.3.6",
    "jest": "^24.8.0",
    "jest-canvas-mock": "^2.1.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "core-js": "^2.6.9",
    "jest": "24.8.0",
    "js-yaml": "^3.13.1",
    "typescript": "3.5.2"
  },
  "jest": {
    "setupFiles": [
      "jest-canvas-mock"
    ],
    "roots": [
      "<rootDir>/src",
      "<rootDir>/test",
      "<rootDir>/dist"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
